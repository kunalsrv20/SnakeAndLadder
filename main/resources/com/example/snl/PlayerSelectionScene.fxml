      </ImageView>
      <ImageView fx:id="loginPlayBtn" fitHeight="54.0" fitWidth="178.0" layoutX="121.0" layoutY="528.0" onMouseClicked="#switchToBoardLoader" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../ProjectImages/playButton.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="54.0" fitWidth="53.0" layoutX="143.0" layoutY="600.0" onMouseClicked="#switchToStart" pickOnBounds="true" preserveRatio="true">
