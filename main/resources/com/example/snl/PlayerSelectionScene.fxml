<AnchorPane prefHeight="669.0" prefWidth="338.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.snl.PlayerController">
